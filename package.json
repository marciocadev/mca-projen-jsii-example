    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.dev.json"
      }
    }
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.marciocadev.projen.jsii.example",
        "maven": {
          "groupId": "com.marciocadev",
          "artifactId": "mca-projen-jsii-example"
        }
      },
      "python": {
        "distName": "mca-projen-jsii-example",
        "module": "mca_projen_jsii_example"
      },
      "dotnet": {
        "namespace": "Marciocadev.Examples",
        "packageId": "Projen.Jsii"
      },
      "go": {
        "moduleName": "github.com/marciocadev/mca-projen-jsii-example-go"
      }
    },
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
